  "files": [
    "dist/*",
    "README.md"
  ],
  "engines": {
    "node": ">=8.6"
  },
  "devDependencies": {
    "@types/fs-extra": "^5.0.5",
    "@types/got": "^9.4.1",
    "@types/jest": "^24.0.11",
    "@types/node": "^11.13.2",
    "jest": "^24.3.1",
    "lint-staged": "^8.1.3",
    "prettier": "^1.16.4",
    "ts-jest": "^24.0.0",
    "typescript": "^3.3.1"
  },
  "lint-staged": {
    "*.ts": [
      "prettier --write \"src/**/*.ts\"",
      "git add"
    ]
  },
  "dependencies": {
    "env-paths": "^2.1.0",
    "fs-extra": "^7.0.1",
    "got": "^9.6.0",
    "sumchecker": "^2.0.2"
